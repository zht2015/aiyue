SQL: INSERT INTO `m_book` (`user_id`,`ISBN`,`bookname`,`press`,`author`,`publication_time`,`price`,`age`,`category`,`booktag`,`content_intro`,`author_intro`,`bookcover`,`list_info`,`c_time`) VALUES ('21','9787544816120','爸爸 妈妈 哥哥和我','接力出版社','(法)杰侯姆·胡里埃著，谢逢蓓译','2011','0','1-2年级','图画书','亲情、想象','《怪味豆哲理图画书:爸爸妈妈哥哥和我》的作者选择绳子的图画造型，形简而意丰。普普通通的一根绳子，有了魔术般神奇的表意功能。温馨甜蜜的团圆时刻，亲密无间的家庭时光，日常琐碎的小小吵闹……在绳子一家的快乐生活中，在丝丝缕缕、牵牵绊绊的联结中，凝结着割也割不断、舍也舍不去的浓浓亲情。',NULL,'0757.jpg',NULL,'2016-10-25 15:40:44') [ RunTime:0.0370s ]
SQL: INSERT INTO `m_book` (`user_id`,`ISBN`,`bookname`,`press`,`author`,`publication_time`,`price`,`age`,`category`,`booktag`,`content_intro`,`author_intro`,`bookcover`,`list_info`,`c_time`) VALUES ('21','9787544816137','黑色的国王','接力出版社','(法)杰侯姆·胡里埃著，谢逢蓓译','2011','0','1-2年级','图画书','创新、想象','《怪味豆哲理图画书:黑色的国王》的作者用简单的图形与颜色向我们讲述了一个有趣而又感人的小故事。一个深刻的主题，被如此形象而简洁地表达出来，让人不得不惊叹于作者化繁为简的艺术功力。',NULL,'0758.jpg',NULL,'2016-10-25 15:40:44') [ RunTime:0.0190s ]
SQL: INSERT INTO `m_book` (`user_id`,`ISBN`,`bookname`,`press`,`author`,`publication_time`,`price`,`age`,`category`,`booktag`,`content_intro`,`author_intro`,`bookcover`,`list_info`,`c_time`) VALUES ('21','9787544816144','小方和小圆','接力出版社','(法)杰侯姆·胡里埃著，谢逢蓓译','2011','0','1-2年级','图画书','创新、想象','《怪味豆哲理图画书:小方和小圆》不论在主题上、故事结构上、构图上、还是所传达的意象上，都体现出一种奇思妙想的创意，体现出一种与众不同的怪趣！',NULL,'0759.jpg',NULL,'2016-10-25 15:40:44') [ RunTime:0.0340s ]
