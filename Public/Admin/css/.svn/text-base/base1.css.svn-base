.pdDetails .tabTitle{ height: 47px; line-height: 47px;background: #f7f7f7; font-size: 14px; color: #767676;border-bottom: solid 1px #ebebeb;}
.pdDetails .tabTitle a{ display: block; float: left; height: 47px; width: 136px; text-align: center; color:#767676; position: relative; }
.pdDetails .tabTitle a i{display: block; position: absolute; top: -1px; width: 100%; height: 3px;}
.pdDetails .tabTitle a.cur{ background: #fff; color: #15a4f0; }
.pdDetails .tabTitle a:hover{ color:#15a4f0 }
.pdDetails .tabTitle a.cur i{ background: #15a4f0; }
.pdDetails .details>li{ display: none;}
.pdDetails .details>li.cur{ display:block;}
.pdDetails .details>li .info{ color: #9e9e9e; padding-bottom: 30px }
.pdDetails .details>li .info h2{ font-size:22px;line-height: 1.5em; }
.pdDetails .details>li .info ul{ width: 25%; float: left;}
.pdDetails .details>li .info ul li{ font-size: 13px; line-height: 36px;}
