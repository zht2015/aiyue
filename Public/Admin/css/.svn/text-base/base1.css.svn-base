@media screen and (max-width: 620px) {
	.header{position: fixed;top:0;left:0;z-index: 100;}
	.header .warp .fl,.header .warp .fr{float: inherit;display: block;}
	.header .top{display: none;}
    .warp{ width:100%; box-sizing: border-box;padding-left: 20px;padding-right: 20px;  }
    .header .warp{width:100%;margin:0;padding:0;}
    .header .logo{width:100%;background-position: 15px center;height:50px;padding:10px;}
    .header .logo .open_nav{display: block}
    .header .warp .nav{margin:0;display: none;}
    .header .fr.search{display: none;}
    .header .warp .nav a{display: block;width:80%;margin:0 auto;float:inherit;text-align: center;background: none;padding:0;line-height: 45px;font-size:1.3rem;color:#888;
		border-top:1px solid #ccc;
    }
    .header .warp .nav span.close_nav{display: block;}
    .footer .warp{ width: 90%; margin-right: auto ; margin-left: auto;}
    .footerWarp{ width:90%;}
    .footer .bottom .ipc{width: 80%}
    .footer .top .footInfo{  margin: 20px auto;width: 100%; }
    .footer .top .nav{ width: 100%; margin: 0 auto;}
    .footer .top .nav dl{ width: 25%;}
    .productListDiv1 .productList{ width:100%; margin-left: auto; margin-right: auto; height:300px;}
    .productListDiv1 .productList ul li{padding:0;margin:0;width: 197px;padding: 0 5px;}
    .productListDiv1 .productList ul li a{width:100%;}
    .productListDiv1 .productList ul li a img{width:100%;height:auto;}
    .productListDiv2 .productList{ width:90%; }
    .productListDiv2 .productList  ul li{ margin:0;padding:0 5px;width:159px; }
    .productListDiv2 .productList  ul li a{width:100%;height:auto;}
    .productListDiv2 .productList  ul li a img{width: 100%;height: auto;}
    .productListDiv2  .tLfetBtn{ left:20px;}
    .productListDiv2  .tRightBtn{ right: 20px;}
    .header{ height:auto;}
    .header .nav{ text-align: center; width: 100%;  }
    .header .nav a{ margin:0 1px; padding:0 5px 0 15px;}
    .banner{ height: 340px;}
    .search{ margin-right: -20px;}
    .banner .sub{ transform: scale(0.8); -webkit-transform: scale(0.8);  }
    .typeInfo{ padding: 30px 0}
    .typeInfo .tLfetBtn,.typeInfo .tRightBtn{ display: none;}
    .typeInfo,.typeInfo .list{ height: auto;}
    .typeInfo .list ul,.typeInfo .list{ width: 100%;}
    .typeInfo .list ul li{ width: 50%; display: inline-block; margin-right: 0;box-sizing: border-box;padding: 0 20px }
    .footer .logoDiv .footNav a{ margin: 0 12px;}
    /*列表页product*/
    .bannerNy{display: none;}
	.nyNav{display: none;}
	.pdTypeBox{display: none;}
	.nyPd{width:100%;}
	.greyBorderLeft{padding:0 10px;}
	.nyProductList{padding:0;}
	.nyProductList ul li{width:50%;margin: 0;}
	.nyProductList ul li a.img img{width:100%;height:auto;}
	.nyProductList ul li a.title{width:90%;padding:0 5px;}
	.nyProductList ul li p{padding:10px 5px;line-height: 25px;}
	.nyProductList ul li a.shoppingCar{position: inherit;margin:0 auto;height:25px;line-height: 25px;right: inherit;bottom:inherit;}
}
